*/node_modules/
*/*/node_modules/

# Build artifacts
dist/
build/
.next/
.turbo/
*.tsbuildinfo

# Development files
.env
.env.local
.env.*.local

# Version control
.git/
.gitignore

# IDE/Editor
.vscode/
.idea/
*.swp
*.swo
*.swn
*.sublime-workspace
*.iml
.DS_Store
.env

# Package manager
bun.lock
yarn.lock
package-lock.json
pnpm-lock.yaml

# Documentation
README.md
guide/

# Other apps (we only need native)
apps/web/
apps/server/

